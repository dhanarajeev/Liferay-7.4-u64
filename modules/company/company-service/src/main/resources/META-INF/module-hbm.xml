		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="gst" type="com.liferay.portal.dao.orm.hibernate.LongType" />
		<property access="com.liferay.portal.dao.orm.hibernate.LiferayPropertyAccessor" name="address" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>